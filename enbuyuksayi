package enbuyuksayi;

import java.util.Scanner;

public class enbuyuksayi {
   public static void main(String[] args) 
   {
	   Scanner baslangic=new Scanner(System.in);
		
		System.out.print("x degeri:");
		int x=baslangic.nextInt();
		
		int enbuyuk=0,sayi;
		for(int i=0;i<x;i++)
		{
			System.out.print((i+1)+". Sayi:");
			sayi=baslangic.nextInt();
			
			if(sayi>enbuyuk)
			{
				enbuyuk=sayi;
			}
		}
		System.out.println("Sayilardan en büyüğü:"+enbuyuk);		
	}	
}
